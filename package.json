  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:check": "npm run copy-pdf-worker && vite build --mode production",
    "build:vercel": "npm run copy-pdf-worker-safe && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "verify-build": "npm run build:check && npm run preview",
    "copy-pdf-worker": "copyfiles -u 3 node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/",
    "copy-pdf-worker-safe": "copyfiles -u 3 node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/ || echo PDF worker copy failed - continuing without it",
    "postinstall": "npm run copy-pdf-worker-safe || echo 'Postinstall completed with warnings'",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean-install": "rm -rf node_modules .npm package-lock.json && npm install --force --include=optional",
    "fix-rollup": "npm install @rollup/rollup-linux-x64-gnu@latest --save-optional --force",
    "build:netlify": "npm run copy-pdf-worker-safe && npm run build:ultra-safe",
    "build:safe": "cross-env NODE_ENV=production vite build --minify esbuild",
    "build:ultra-safe": "vite build --mode production --minify esbuild --config vite.config.safe.ts"
  },
